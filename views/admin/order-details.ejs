    <script>
        // Add a click event listener to the dropdown items

        document.querySelectorAll('.dropdown-item').forEach(function(item) {
            item.addEventListener('click', function() {
                var selectedItemText = item.innerHTML;
                
                // var id = orders[0]._id
                console.log('Selected Item: ' + selectedItemText);
                const id = '<%- orders[0]._id %>'
                $.ajax({
                    method:"post",
                    url : "/admin/statusupdate",
                    data : {
                        data :selectedItemText,
                        orderid:id
                    },
                    success: (response)=>{
                        if(response.status===true){
                            location.reload()
                        }
                    }
                })
                
            });
        });
    </script>
