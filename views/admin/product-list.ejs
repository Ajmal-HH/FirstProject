        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                </div>
                <div>
                    <a href="#" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col col-check flex-grow-0">
                            
                        </div>
                       
                        
                     
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <% for(let i=0 ; i < product.length ; i++) { %>
                    <article class="itemlist">
                        
                        <div class="row align-items-center">
                            <div class="col col-check flex-grow-0">
                                <div class="form-check">
                                    <!-- <input class="form-check-input" type="checkbox"> -->
                                    <p><%= i+1  %></p>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-8 flex-grow-1 col-name">
                                <a class="itemside" href="#">
                                    <div class="left">
                                        <img src="admin-assets/uploads/<%= product[i].image[0]  %>" class="img-sm img-thumbnail" alt="Item">
                                    </div>
                                    <div class="info">
                                        <h6 class="mb-0"><%= product[i].product_name  %></h6>
                                    </div>
                                </a>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-price"> <span><%= product[i].offer_price  %></span> </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-status">
                                <% if(product[i].is_listed === true){ %>
                                <span class="badge rounded-pill alert-danger">Inactive</span>
                                <% }else{ %>
                                    <span class="badge rounded-pill alert-success">Active</span>

                                    <% } %>
                            </div>
                            <div class="col-lg-1 col-sm-2 col-4 col-date">
                                <span><%= product[i].stock  %>Units</span>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-action ">
                                <a href="/admin/editproduct?id=<%= product[i]._id  %>" class="btn btn-sm font-sm rounded btn-brand text-end">
                                    <i class="material-icons md-edit"></i> Edit
                                </a>
                                <% if(product[i].is_listed === true) { %>
                                    <a href="/admin/unlistproduct/?id=<%= product[i]._id  %>" class="btn btn-danger btn-sm   rounded font-sm mt-8 ">UnList</a>

                                    <% }else { %>
                                        <a href="/admin/listproduct/?id=<%= product[i]._id  %>" class="btn btn-sm btn-brand rounded font-sm mt-8 ">List</a>

                                        <% } %>

                            </div>
                        </div> <!-- row .// -->
                    </article> <!-- itemlist  .// -->
                    <% } %>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
            <div class="pagination-area mt-30 mb-50">
              <a href="/admin/productlist?page=<%= page-1 %>"><button class="btn btn-primary">< Previous</button></a>
              <a href="/admin/productlist?page=<%= page+1 %> "><button class="btn btn-primary">Next ></button></a>
            </div>
        </section> <!-- content-main end// -->
