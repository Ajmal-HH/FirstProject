    <script>
        function validateInput(input) {
          if (input.value.trim() === '') {
            input.setCustomValidity("Please enter a valid value.");
          } else {
            input.setCustomValidity('');
          }
        }


        function validation(){
            //To clear previous error message
            document.getElementById('product_name_error').textContent = '';
            document.getElementById('og_price_error').textContent = '';
            document.getElementById('offer_price_error').textContent = '';
            document.getElementById('stock_error').textContent = '';
            document.getElementById('image_error').textContent = '';

            let product_name = document.getElementById('product_name').value;
            let og_price = document.getElementById('og_price').value;
            let offer_price = document.getElementById('offer_price').value;
            let stock = document.getElementById('stock').value;
            let image = document.querySelector('input[name="image"]').files;

            let isValid = true

            if (product_name.trim() === '') {
                document.getElementById('product_name_error').textContent = 'Product Name is required';
                isValid = false;
            }
            if(og_price <= 0) {
                document.getElementById('og_price_error').textContent = 'Please Enter Valid Price';
                isValid = false;

            }
            if(offer_price<=0 || offer_price>og_price){
                document.getElementById('offer_price_error').textContent = 'Please Enter Valid Price';
                isValid = false;

            }
            if(stock.trim() === '' || stock<0){
                document.getElementById('stock_error').textContent = 'Please Enter Valid Value';
                isValid = false;

            }
            if(image.length === 0){
                document.getElementById('image_error').textContent = 'Please select an Image';
                isValid = false;
            }

            if(isValid){
                return true
            }else{
                return false
            }

        }

      </script>


